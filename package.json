{
  "name": "dungeon-revealer",
  "version": "1.0.0",
  "scripts": {
    "start": "node ./bin/dungeon-revealer",
    "eslint": "./node_modules/.bin/eslint --ignore-path .gitignore \"**/*.js\"",
    "start:dev": "cross-env PORT=3001 react-scripts start",
    "build": "react-scripts build && pkg . --out-path ./bin/ --targets node10-win-x64,node10-macos-x64,node10-linux-x64",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "postinstall": "npm run build"
  },
  "dependencies": {
    "@emotion/core": "10.0.14",
    "@emotion/styled": "10.0.11",
    "body-parser": "~1.19.0",
    "body-scroll-lock": "2.6.3",
    "connect-busboy": "0.0.2",
    "cookie-parser": "~1.4.4",
    "cookie-session": "~1.3.3",
    "debug": "~4.1.1",
    "express": "~4.17.0",
    "express-basic-auth": "1.2.0",
    "fs-extra": "8.0.1",
    "junk": "3.1.0",
    "morgan": "~1.9.1",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-easy-panzoom": "0.3.0",
    "react-tooltip": "3.10.0",
    "referentiel": "0.0.8",
    "serve-favicon": "~2.5.0",
    "socket.io": "^2.2.0",
    "socket.io-client": "2.2.0",
    "use-persisted-state": "0.3.0",
    "uuid": "3.3.2"
  },
  "description": "dungeon-revealer ================",
  "main": "app.js",
  "devDependencies": {
    "cross-env": "5.2.0",
    "eslint": "5.16.0",
    "eslint-config-prettier": "5.0.0",
    "husky": "2.7.0",
    "lint-staged": "8.2.1",
    "pkg": "^4.3.7",
    "prettier": "1.18.2",
    "react-scripts": "3.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/apclary/dungeon-revealer.git"
  },
  "author": "apclary",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/apclary/dungeon-revealer/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "*.{js,json,md,html}": [
        "prettier --write",
        "git add"
      ],
      "*.js": [
        "eslint --fix"
      ]
    }
  },
  "pkg": {
    "assets": [
      "build/**/*"
    ]
  },
  "bin": "bin/dungeon-revealer",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
